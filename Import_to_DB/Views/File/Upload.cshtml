@{  
    Layout = null;  
}  
   
<!DOCTYPE html>  
   
<html>  
<head>  
    <meta name="viewport" content="width=device-width"/>  
    <title>Index</title>  
</head>  
<body>
<!--Upload and reload page-->
    @if (ViewBag.Message != null)  
    {  
        <script type="text/javascript">  
            window.onload = function () {  
                alert("@ViewBag.Message");
                document.location = "http://localhost:65149";
            };  
        </script>  
    }  
</body>  
</html>  